CREATE DATABASE ttest
CREATE TABLE `ttest`.`comments` ( `id` int(11) NOT NULL, `postId` int(9) NOT NULL, `name` varchar(255) NOT NULL, `email` varchar(255) NOT NULL, `body` varchar(255) NOT NULL ) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4; ALTER TABLE `ttest`.`comments` ADD PRIMARY KEY (`id`), ADD KEY `postId` (`postId`); ALTER TABLE `ttest`.`comments` MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
CREATE TABLE `ttest`.`records` ( `id` int(11) NOT NULL, `userId` int(9) NOT NULL, `title` varchar(255) NOT NULL, `body` varchar(255) NOT NULL ) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4; ALTER TABLE `ttest`.`records` ADD PRIMARY KEY (`id`); ALTER TABLE `ttest`.`records` MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
